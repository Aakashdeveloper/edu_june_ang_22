var city = ['Delhi', 'Mumbai', 'London', 'Amsterdam', 'Venice', 'Boston']
city.indexOf('Delhi')
0
city.indexOf('Venice')
4
city.indexOf('Helsinki')
-1
city.indexOf('Pune')
-1

